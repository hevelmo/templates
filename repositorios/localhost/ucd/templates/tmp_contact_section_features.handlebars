<div id="section-contact" class="section bg-white">
    <div class="container">
        <div class="section-heading text-center">
            <div class="title">Nuestra ubicación</div>
            <div class="line"></div>
        </div>
    </div>
    <div style="overflow:hidden;height:775px;width:100%;">
        <div id="gmap_canvas" class="map-holder" style="height:775px;width:100%;"></div>
    </div>
    <div class="container">
        <div class="section-content">
            <div class="row pb-70">
                <div class="col-md-6 col-sm-6">
                    <div class="box mbn">
                        <div class="box-heading">Envíanos un mensaje</div>
                        <div class="box-body form-contact" id="send_form_wrapper">
                            <form class="form-contact" id="send_ucd_form_contact">
                                <div class="form-group">
                                    <label class="control-label mll">
                                        <span class="required">
                                            <i class="fa fa-asterisk"></i>
                                        </span>
                                        Nombre
                                    </label>
                                    <input type="text" name="send_ucd_contact_get_name" id="contact-get-name" class="form-control"></div>
                                <div class="form-group">
                                    <label class="control-label mll">
                                        <span class="required">
                                            <i class="fa fa-asterisk"></i>
                                        </span>
                                        Correo
                                    </label>
                                    <input type="text" name="send_ucd_contact_get_email" id="contact-get-email" class="form-control"></div>
                                <div class="form-group">
                                    <label class="control-label mll">
                                        Asunto
                                    </label>
                                    <input type="text" name="send_ucd_contact_get_subject" id="contact-get-subject" class="form-control"></div>
                                <div class="form-group">
                                    <label class="control-label mll">
                                        Mensaje
                                    </label>
                                    <textarea rows="8" name="send_ucd_contact_get_message" id="contact-get-message" class="form-control"></textarea>
                                </div>
                            </form>
                            <div class="form-group mtxxl text-center mbn">
                                <input type="button" value="Enviar Mensaje" name="send_contact" id="ucd_send_contact_submit" class="btn btn-outlined btn-primary">
                            </div>
                        </div>
                        <div id="send_message_form_thanks" class="form-thanks" style="display: none;">
                            <p class="agradecimiento_titulo">Mensaje enviado</p>
                            <!--<hr>-->
                            <p class="agradecimiento_titulo2">En breve responderemos tu mensaje al siguiente correo
                                <br>
                                <span id="email-from">-</span>
                            </p>
                        </div>
                        <div id="send_message_form_error" class="form-error" style="display: none;">
                            <p class="agradecimiento_titulo">Hubo un error</p>
                            <!--<hr>-->
                            <p class="agradecimiento_titulo2">Intentalo nuevamente</p>
                        </div>
                        <div id="send_message_form_loader" class="form-loader" style="display: none;">
                            <div class="loader">
                                <div class="loader-wrap">
                                    <div class="loader-item"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-sm-6">
                    <div class="box mbn">
                        <div class="box-heading">Contáctanos</div>
                        <div class="box-body">
                            <p>
                                UCD Unidad Clínica Diagnóstica.
                            </p>
                            <p>
                                Laboratorio Central.
                            </p>

                            <div class="contact-infos">
                                <ul class="list-unstyled">
                                    <li>
                                        <a>
                                            <i class="fa fa-map-marker fa-fw"></i>
                                            Av. Hidalgo #2016 Col. Ladrón de Guevara, Guadalajara, Jalisco
                                        </a>
                                    </li>
                                    <li>
                                        <a>
                                            <i class="fa fa-phone fa-fw"></i>
                                            <span class="span-unstyled">
                                                01 (33) 1588 2223,
                                                01 (33) 8995 2325,
                                                01 (33) 8995 0527 {{!--<br>
                                                01 (33) 1809 8643 <br>
                                                01 (33) 1809 8644--}}
                                            </span>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="mailto:info@ucd.com.mx?subject=feedback" "correo">
                                            <i class="fa fa-envelope fa-fw"></i>
                                            info@ucd.com.mx
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            <!--<a href="#"><img src="assets/images/banner_faq.jpg" alt="" class="img-responsive"/></a></div>-->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
